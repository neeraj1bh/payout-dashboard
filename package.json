{
  "name": "payout-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier .",
    "prettier:check": "yarn prettier --check",
    "prettier:write": "yarn prettier --write",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "yarn lint"
    ],
    "src/**/*.{ts,tsx, js,jsx,json,md,css,scss,html,yml,yaml,xml}": [
      "yarn prettier:write"
    ]
  },
  "dependencies": {
    "axios": "^1.6.7",
    "moment": "^2.30.1",
    "next": "14.1.2",
    "react": "^18",
    "react-dom": "^18",
    "react-select": "^5.8.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.1.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5"
  }
}
